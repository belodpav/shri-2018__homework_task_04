{
  "name": "github-viewer",
  "version": "1.0.0",
  "description": "github viewer",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "mocha test/*",
    "clear": "rimraf dist",
    "server": "nodemon app.js",

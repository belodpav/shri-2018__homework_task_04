doctype html
html
  head
    link(rel="icon" href="/static/favicon.ico" type="image/x-icon")
    link(rel='stylesheet', href='/static/index.min.css')
  body.page.font.font_face_ss
    header.header.page__header
        a.logo(href="/", title="GitTub") GitTub
        a(href="/commits/" + branch, title="commits") Commits
        a(href="/branches/", title="Branches") Branches
    ul
        if parentUrl
            li
                a(href=parentUrl title="Назад") ..
        each val in list
            li
                if val.type === 'tree'
                    a(href=url + val.name title= val.name)= val.name
                else
                    a(href='/blob' + url + val.name title= val.name)= val.name
    script(src="/static/index.min.js")